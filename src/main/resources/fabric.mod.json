{
  "schemaVersion": 1,
  "id": "autumnity",
  "version": "${version}",

  "name": "Autumnity",
  "description": "An autumn-themed mod with new biomes and friendly snails.",
  "authors": [
    "Mr.George"
  ],
  "contact": {
    "homepage": "https://github.com/Mr-George-star/",
    "sources": "https://github.com/Mr-George-star/autumnity-fabric",
    "issues": "https://github.com/Mr-George-star/autumnity-fabric/issues"
  },

  "license": "MIT Licence",
  "icon": "assets/autumnity/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.george.autumnity.Autumnity"
    ],
    "client": [
      "net.george.autumnity.AutumnityClient"
    ],
    "terrablender": [
      "net.george.autumnity.world.gen.AutumnityBiomeGeneration"
    ],
    "fabric-datagen": [
      "net.george.autumnity.datagen.AutumnityDataGeneration"
    ]
  },
  "mixins": [
    "autumnity.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.18.2",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
